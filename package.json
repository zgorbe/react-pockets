{
  "name": "react-pockets",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.3",
    "bootstrap": "^4.1.3",
    "firebase": "^5.5.1",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.4",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-bar-chart": "^0.1.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "reactstrap": "^6.4.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
