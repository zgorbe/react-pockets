{
  "name": "react-pockets",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.3",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.9.3",
    "firebase": "^7.14.2",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.5.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.4.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
